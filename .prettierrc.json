{
	"tabWidth": 4,
	"useTabs": true,
	"semi": false,
	"printWidth": 120,
	"trailingComma": "none",
	"arrowParens": "avoid",
	"htmlWhitespaceSensitivity": "ignore",
	"plugins": [
		"prettier-plugin-tailwindcss"
	],
	"tailwindAttributes": [
		"class",
		"container-class",
		"header-class",
		"pane-wrapper-class",
		"trigger-class",
		"tab-class",
		"content-class"
	]
}
