    "jose": "^6.1.2",
    "lodash": "^4.17.21",
    "mitt": "^3.0.1",
    "naive-ui": "^2.43.2",
    "nanoid": "^5.1.6",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "secure-ls": "^2.0.0",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
