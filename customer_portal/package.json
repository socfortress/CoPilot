{
  "name": "copilot-customer-portal",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.17.0+sha512.fce8a3dd29a4ed2ec566fb53efbb04d8c44a0f05bc6f24a73046910fb9c3ce7afa35a0980500668fa3573345bd644644fa98338fa168235c80f4aa17aa17fbef",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3001",
    "build": "run-p type-check \"build:only {@}\" --",
    "build:only": "vite build",
    "build:docker": "docker build .",
    "preview": "vite build && echo '' && vite preview --port 4174 --host",
    "preview:only": "vite preview --port 4174 --host",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "start-server": "cd ../backend && /opt/venv/bin/python copilot.py",
    "start-frontend": "vite --host 0.0.0.0 --port 3001",
    "start": "concurrently \"pnpm start-server\" \"pnpm start-frontend\""
  },
  "dependencies": {
    "@ajoelp/json-to-formdata": "^1.5.0",
