{
	"name": "copilot",
	"version": "1.0.0",
	"private": true,
	"overrides": {
		"secure-ls": {
			"crypto-js": "^4.2.0"
		}
	},
	"scripts": {
		"dev": "vite --host 0.0.0.0",
		"dev:debug": "DEBUG=vite:* vite",
		"build": "run-p type-check build-only",
		"build-only": "vite build",
		"build-analyzer": "vite-bundle-visualizer -t sunburst",
		"preview": "vite build && echo '' && vite preview --port 4173 --host",
		"preview-only": "vite preview --port 4173 --host",
		"test:unit": "vitest --environment jsdom",
		"test:e2e": "start-server-and-test preview http://localhost:4173/ 'cypress open --e2e'",
		"test:e2e:ci": "start-server-and-test preview http://localhost:4173/ 'cypress run --e2e'",
		"type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
		"lint": "eslint . --ext .vue,.ts,.tsx,.cts,.mts --fix",
		"tailwind-config-viewer": "tailwind-config-viewer -o",
		"design-tokens": "node scripts/tokens-tool.js",
		"start-server-old": "cd ../backend && uvicorn copilot:app --reload --port=5000",
		"start-server-venv": "cd ../backend && source .venv/bin/activate && uvicorn copilot:app --port=5000 --log-level debug",
		"start-server": "cd ../backend && /opt/venv/bin/python copilot.py",
		"start-vue": "vite --host 0.0.0.0",
		"start": "concurrently \"npm run start-server\" \"npm run start-vue\"",
		"libs-check": "taze",
		"libs-reload": "rm -rf node_modules package-lock.json && npm install",
		"open:swagger": "open http://127.0.0.1:5000/docs#/",
		"open:redoc": "open http://127.0.0.1:5000/redoc",
		"prettify": "prettier --write ."
	},
	"dependencies": {
		"@ajoelp/json-to-formdata": "^1.5.0",
		"@f3ve/vue-markdown-it": "^0.2.2",
		"@fontsource/jetbrains-mono": "^5.0.20",
		"@fontsource/lexend": "^5.0.20",
		"@fontsource/public-sans": "^5.0.18",
		"@popperjs/core": "^2.11.8",
		"@shikijs/markdown-it": "^1.6.1",
		"@vueuse/components": "^10.10.0",
		"@vueuse/core": "^10.10.0",
		"apexcharts": "^3.49.1",
		"bytes": "^3.1.2",
		"colord": "^2.9.3",
		"crypto-js": "^4.2.0",
		"dayjs": "^1.11.11",
		"detect-touch-device": "^1.1.6",
		"echarts": "^5.5.0",
		"file-saver": "^2.0.5",
		"html-entities": "^2.5.2",
		"jose": "^5.3.0",
		"js-md5": "^0.8.3",
		"lodash": "^4.17.21",
		"mitt": "^3.0.1",
		"naive-ui": "^2.38.2",
		"password-validator": "^5.3.0",
		"pinia": "^2.1.7",
		"pinia-plugin-persistedstate": "^3.2.1",
		"secure-ls": "^1.2.6",
		"shiki": "^1.6.1",
		"validator": "^13.12.0",
		"vue": "^3.4.27",
		"vue-advanced-cropper": "^2.8.8",
		"vue-highlight-words": "^3.0.1",
		"vue-i18n": "^9.13.1",
		"vue-router": "^4.3.2",
		"vue-sjv": "^0.0.6",
		"vue3-apexcharts": "^1.5.3",
		"vue3-marquee": "^4.2.0",
		"vuedraggable": "^4.1.0"
	},
	"devDependencies": {
		"@clack/prompts": "^0.7.0",
		"@iconify/vue": "^4.1.2",
		"@rushstack/eslint-patch": "^1.10.3",
		"@tsconfig/node18": "^18.2.4",
		"@types/bytes": "^3.1.4",
		"@types/file-saver": "^2.0.7",
		"@types/fs-extra": "^11.0.4",
		"@types/inquirer": "^9.0.7",
		"@types/jsdom": "^21.1.6",
		"@types/lodash": "^4.17.4",
		"@types/node": "^20.12.13",
		"@types/validator": "^13.11.10",
		"@vitejs/plugin-vue": "^5.0.4",
		"@vitejs/plugin-vue-jsx": "^3.1.0",
		"@vue/eslint-config-prettier": "^9.0.0",
		"@vue/eslint-config-typescript": "^13.0.0",
		"@vue/test-utils": "^2.4.6",
		"@vue/tsconfig": "^0.5.1",
		"autoprefixer": "^10.4.19",
		"cypress": "^13.10.0",
		"eslint": "^8.57.0",
		"eslint-plugin-cypress": "^3.3.0",
		"eslint-plugin-vue": "^9.26.0",
		"flourite": "^1.2.4",
		"fs-extra": "^11.2.0",
		"ip": "^2.0.1",
		"jsdom": "^24.1.0",
		"json5": "^2.2.3",
		"npm-run-all": "^4.1.5",
		"picocolors": "^1.0.1",
		"postcss": "^8.4.38",
		"prettier": "^3.2.5",
		"sass": "^1.77.3",
		"start-server-and-test": "^2.0.3",
		"tailwind-config-viewer": "^2.0.2",
		"tailwindcss": "^3.4.3",
		"taze": "^0.13.8",
		"unplugin-vue-components": "^0.27.0",
		"vite": "^5.2.12",
		"vite-bundle-analyzer": "^0.9.4",
		"vite-bundle-visualizer": "^1.2.1",
		"vite-svg-loader": "^5.1.0",
		"vitest": "^1.6.0",
		"vue-tsc": "^2.0.19"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}