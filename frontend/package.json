{
	"name": "copilot",
	"type": "module",
	"version": "1.0.0",
	"private": true,
	"packageManager": "pnpm@10.4.1",
	"engines": {
		"node": ">=18.0.0"
	},
	"scripts": {
		"dev": "vite --host 0.0.0.0",
		"dev:debug": "DEBUG=vite:* vite",
		"build": "run-p type-check \"build:only {@}\" --",
		"build:only": "vite build",
		"build:analyzer": "vite-bundle-visualizer -t sunburst",
		"build:check": "run-s libs:reload build lint type-check format test:e2e test:unit",
		"preview": "vite build && echo '' && vite preview --port 4173 --host",
		"preview:only": "vite preview --port 4173 --host",
		"test:unit": "vitest run",
		"test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
		"test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
		"type-check": "vue-tsc --build --force",
		"lint": "eslint . --fix",
		"format": "prettier --write src/",
		"lint-format": "run-s lint type-check format",
		"design-tokens": "node scripts/tokens-tool.js",
		"start-server:old": "cd ../backend && uvicorn copilot:app --reload --port=5000",
		"start-server:venv": "cd ../backend && source .venv/bin/activate && uvicorn copilot:app --port=5000 --log-level debug",
		"start-server": "cd ../backend && /opt/venv/bin/python copilot.py",
		"start-frontend": "vite --host 0.0.0.0",
		"start": "concurrently \"pnpm start-server\" \"pnpm start-frontend\"",
		"libs:check": "taze && depcheck",
		"libs:reload": "rm -rf node_modules pnpm-lock.yaml && pnpm install",
		"open:swagger": "open http://127.0.0.1:5000/docs#/",
		"open:redoc": "open http://127.0.0.1:5000/redoc"
	},
	"dependencies": {
		"@ajoelp/json-to-formdata": "^1.5.0",
		"@f3ve/vue-markdown-it": "^0.2.3",
		"@fontsource/jetbrains-mono": "^5.2.5",
		"@fontsource/lexend": "^5.2.5",
		"@fontsource/public-sans": "^5.2.5",
		"@shikijs/markdown-it": "^3.1.0",
		"@tailwindcss/container-queries": "^0.1.1",
		"@vueuse/core": "^12.8.1",
		"axios": "^1.8.1",
		"bytes": "^3.1.2",
		"colord": "^2.9.3",
		"dayjs": "^1.11.13",
		"detect-touch-device": "^1.1.6",
		"echarts": "^5.6.0",
		"file-saver": "^2.0.5",
		"html-entities": "^2.5.2",
		"jose": "^6.0.8",
		"js-md5": "^0.8.3",
		"lodash": "^4.17.21",
		"mitt": "^3.0.1",
		"naive-ui": "^2.41.0",
		"nanoid": "^5.1.2",
		"password-validator": "^5.3.0",
		"pinia": "^3.0.1",
		"pinia-plugin-persistedstate": "^4.2.0",
		"secure-ls": "^2.0.0",
		"shiki": "^3.1.0",
		"validator": "^13.12.0",
		"vue": "^3.5.13",
		"vue-advanced-cropper": "^2.8.9",
		"vue-highlight-words": "^3.0.1",
		"vue-i18n": "^11.1.1",
		"vue-router": "^4.5.0",
		"vue-sjv": "^0.0.6",
		"vue3-apexcharts": "^1.8.0",
		"vue3-marquee": "^4.2.2",
		"vuedraggable": "^4.1.0"
	},
	"optionalDependencies": {
		"@rollup/rollup-linux-x64-gnu": "^4.34.8",
		"treemate": "^0.3.11",
		"vueuc": "^0.4.64"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^4.5.1",
		"@clack/prompts": "^0.10.0",
		"@iconify/vue": "^4.3.0",
		"@tsconfig/node20": "^20.1.4",
		"@types/bytes": "^3.1.5",
		"@types/file-saver": "^2.0.7",
		"@types/fs-extra": "^11.0.4",
		"@types/jsdom": "^21.1.7",
		"@types/lodash": "^4.17.16",
		"@types/node": "^22.13.9",
		"@types/validator": "^13.12.2",
		"@vitejs/plugin-vue": "^5.2.1",
		"@vitejs/plugin-vue-jsx": "^4.1.1",
		"@vue/test-utils": "^2.4.6",
		"@vue/tsconfig": "^0.7.0",
		"autoprefixer": "^10.4.20",
		"cypress": "^14.1.0",
		"depcheck": "^1.4.7",
		"eslint": "^9.21.0",
		"flourite": "^1.3.0",
		"fs-extra": "^11.3.0",
		"jsdom": "^26.0.0",
		"npm-run-all2": "^7.0.2",
		"postcss": "^8.5.3",
		"prettier": "^3.5.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"sass": "^1.85.1",
		"start-server-and-test": "^2.0.10",
		"tailwindcss": "^3.4.17",
		"taze": "^18.6.0",
		"type-fest": "^4.37.0",
		"typescript": "~5.8.2",
		"vite": "^6.2.0",
		"vite-bundle-visualizer": "^1.2.1",
		"vite-plugin-vue-devtools": "^7.7.2",
		"vite-svg-loader": "^5.1.0",
		"vitest": "^3.0.7",
		"vue-tsc": "^2.2.8"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@parcel/watcher",
			"cypress",
			"esbuild"
		]
	}
}
