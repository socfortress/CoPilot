{
	"name": "copilot",
	"type": "module",
	"version": "1.0.0",
	"private": true,
	"packageManager": "pnpm@10.9.0+sha512.0486e394640d3c1fb3c9d43d49cf92879ff74f8516959c235308f5a8f62e2e19528a65cdc2a3058f587cde71eba3d5b56327c8c33a97e4c4051ca48a10ca2d5f",
	"engines": {
		"node": ">=18.0.0"
	},
	"scripts": {
		"dev": "vite --host 0.0.0.0",
		"dev:debug": "DEBUG=vite:* vite",
		"build": "run-p type-check \"build:only {@}\" --",
		"build:only": "vite build",
		"build:docker": "docker build .",
		"build:analyzer": "vite-bundle-visualizer -t sunburst",
		"build:check": "run-s libs:reload build lint type-check format test:e2e test:unit build:docker",
		"preview": "vite build && echo '' && vite preview --port 4173 --host",
		"preview:only": "vite preview --port 4173 --host",
		"test:unit": "vitest run",
		"test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
		"test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
		"type-check": "vue-tsc --build --force",
		"lint": "eslint . --fix",
		"format": "prettier --write src/",
		"lint-type-format": "run-s lint type-check format",
		"design-tokens": "node scripts/tokens-tool.js",
		"start-server:old": "cd ../backend && uvicorn copilot:app --reload --port=5000",
		"start-server:venv": "cd ../backend && source .venv/bin/activate && uvicorn copilot:app --port=5000 --log-level debug",
		"start-server": "cd ../backend && /opt/venv/bin/python copilot.py",
		"start-frontend": "vite --host 0.0.0.0",
		"start": "concurrently \"pnpm start-server\" \"pnpm start-frontend\"",
		"libs:check": "taze && depcheck",
		"libs:reload": "rm -rf node_modules pnpm-lock.yaml && pnpm install",
		"open:swagger": "open http://127.0.0.1:5000/docs#/",
		"open:redoc": "open http://127.0.0.1:5000/redoc"
	},
	"dependencies": {
		"@ajoelp/json-to-formdata": "^1.5.0",
		"@codemirror/commands": "^6.8.1",
		"@codemirror/lang-javascript": "^6.2.3",
		"@codemirror/lang-xml": "^6.1.0",
		"@codemirror/theme-one-dark": "^6.1.2",
		"@f3ve/vue-markdown-it": "^0.2.3",
		"@fontsource/jetbrains-mono": "^5.2.5",
		"@fontsource/lexend": "^5.2.6",
		"@fontsource/public-sans": "^5.2.5",
		"@shikijs/markdown-it": "^3.3.0",
		"@vueuse/core": "^13.1.0",
		"axios": "^1.9.0",
		"bytes": "^3.1.2",
		"codemirror": "~6.0.1",
		"colord": "^2.9.3",
		"dayjs": "^1.11.13",
		"detect-touch-device": "^1.1.6",
		"echarts": "^5.6.0",
		"file-saver": "^2.0.5",
		"html-entities": "^2.6.0",
		"jose": "^6.0.10",
		"js-md5": "^0.8.3",
		"lodash": "^4.17.21",
		"mitt": "^3.0.1",
		"naive-ui": "^2.41.0",
		"nanoid": "^5.1.5",
		"password-validator": "^5.3.0",
		"pinia": "^3.0.2",
		"pinia-plugin-persistedstate": "^4.2.0",
		"secure-ls": "^2.0.0",
		"shiki": "^3.3.0",
		"thememirror": "^2.0.1",
		"validator": "^13.15.0",
		"vue": "^3.5.13",
		"vue-advanced-cropper": "^2.8.9",
		"vue-codemirror": "^6.1.1",
		"vue-highlight-words": "^3.0.1",
		"vue-i18n": "^11.1.3",
		"vue-router": "^4.5.1",
		"vue-sjv": "^0.0.6",
		"vue3-apexcharts": "^1.8.0",
		"vue3-marquee": "^4.2.2",
		"vuedraggable": "^4.1.0"
	},
	"optionalDependencies": {
		"@rollup/rollup-linux-x64-gnu": "^4.40.0",
		"treemate": "^0.3.11",
		"vueuc": "^0.4.64"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^4.12.0",
		"@clack/prompts": "^0.10.1",
		"@iconify/vue": "^4.3.0",
		"@tailwindcss/vite": "^4.1.4",
		"@tsconfig/node20": "^20.1.5",
		"@types/bytes": "^3.1.5",
		"@types/file-saver": "^2.0.7",
		"@types/fs-extra": "^11.0.4",
		"@types/jsdom": "^21.1.7",
		"@types/lodash": "^4.17.16",
		"@types/node": "^22.15.2",
		"@types/validator": "^13.15.0",
		"@vitejs/plugin-vue": "^5.2.3",
		"@vitejs/plugin-vue-jsx": "^4.1.2",
		"@vue/test-utils": "^2.4.6",
		"@vue/tsconfig": "^0.7.0",
		"cypress": "^14.3.2",
		"depcheck": "^1.4.7",
		"eslint": "^9.25.1",
		"flourite": "^1.3.0",
		"fs-extra": "^11.3.0",
		"jsdom": "^26.1.0",
		"npm-run-all2": "^7.0.2",
		"prettier": "^3.5.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"sass": "^1.87.0",
		"start-server-and-test": "^2.0.11",
		"tailwindcss": "^4.1.4",
		"taze": "^19.0.4",
		"type-fest": "^4.40.0",
		"typescript": "~5.8.3",
		"vite": "^6.3.3",
		"vite-bundle-visualizer": "^1.2.1",
		"vite-plugin-vue-devtools": "^7.7.5",
		"vite-svg-loader": "^5.1.0",
		"vitest": "^3.1.2",
		"vue-tsc": "^2.2.10"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@parcel/watcher",
			"cypress",
			"esbuild",
			"unrs-resolver"
		]
	}
}
