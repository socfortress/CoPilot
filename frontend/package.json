		"prettier": "^3.3.3",
		"sass": "^1.77.8",
		"start-server-and-test": "^2.0.5",
		"tailwind-config-viewer": "^2.0.4",
		"tailwindcss": "^3.4.10",
		"taze": "^0.16.7",
		"type-fest": "^4.26.0",
		"unplugin-vue-components": "^0.27.4",
		"vite": "^5.4.2",
		"vite-bundle-analyzer": "^0.10.6",
		"vite-bundle-visualizer": "^1.2.1",
		"vite-plugin-vue-devtools": "^7.3.9",
		"vite-svg-loader": "^5.1.0",
