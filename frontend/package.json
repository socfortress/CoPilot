{
	"name": "copilot",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite --host 0.0.0.0",
		"dev:debug": "DEBUG=vite:* vite",
		"build": "run-p type-check \"build-only {@}\" --",
		"build-only": "vite build",
		"build-analyzer": "vite-bundle-visualizer -t sunburst",
		"preview": "vite build && echo '' && vite preview --port 4173 --host",
		"preview-only": "vite preview --port 4173 --host",
		"test:unit": "vitest",
		"test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
		"test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
		"type-check": "vue-tsc --build --force",
		"lint": "eslint . --fix",
		"tailwind-config-viewer": "tailwind-config-viewer -o",
		"design-tokens": "node scripts/tokens-tool.js",
		"start-server-old": "cd ../backend && uvicorn copilot:app --reload --port=5000",
		"start-server-venv": "cd ../backend && source .venv/bin/activate && uvicorn copilot:app --port=5000 --log-level debug",
		"start-server": "cd ../backend && /opt/venv/bin/python copilot.py",
		"start-vue": "vite --host 0.0.0.0",
		"start": "concurrently \"npm run start-server\" \"npm run start-vue\"",
		"libs-check": "taze",
		"libs-reload": "rm -rf node_modules package-lock.json && npm install",
		"open:swagger": "open http://127.0.0.1:5000/docs#/",
		"open:redoc": "open http://127.0.0.1:5000/redoc",
		"format": "prettier --write src/"
	},
	"dependencies": {
		"@ajoelp/json-to-formdata": "^1.5.0",
		"@f3ve/vue-markdown-it": "^0.2.2",
		"@fontsource/jetbrains-mono": "^5.0.21",
		"@fontsource/lexend": "^5.0.21",
		"@fontsource/public-sans": "^5.0.18",
		"@popperjs/core": "^2.11.8",
		"@shikijs/markdown-it": "^1.16.1",
		"@vueuse/components": "^11.0.3",
		"@vueuse/core": "^11.0.3",
		"apexcharts": "^3.53.0",
		"axios": "^1.7.7",
		"bytes": "^3.1.2",
		"colord": "^2.9.3",
		"crypto-js": "^4.2.0",
		"dayjs": "^1.11.13",
		"detect-touch-device": "^1.1.6",
		"echarts": "^5.5.1",
		"file-saver": "^2.0.5",
		"html-entities": "^2.5.2",
		"jose": "^5.8.0",
		"js-md5": "^0.8.3",
		"lodash": "^4.17.21",
		"mitt": "^3.0.1",
		"naive-ui": "^2.39.0",
		"password-validator": "^5.3.0",
		"pinia": "^2.2.2",
		"pinia-plugin-persistedstate": "^3.2.3",
		"secure-ls": "^2.0.0",
		"shiki": "^1.16.1",
		"validator": "^13.12.0",
		"vue": "^3.4.38",
		"vue-advanced-cropper": "^2.8.9",
		"vue-highlight-words": "^3.0.1",
		"vue-i18n": "^9.14.0",
		"vue-router": "^4.4.3",
		"vue-sjv": "^0.0.6",
		"vue3-apexcharts": "^1.6.0",
		"vue3-marquee": "^4.2.2",
		"vuedraggable": "^4.1.0"
	},
	"devDependencies": {
		"@clack/prompts": "^0.7.0",
		"@iconify/vue": "^4.1.2",
		"@tsconfig/node20": "^20.1.4",
		"@types/bytes": "^3.1.4",
		"@types/file-saver": "^2.0.7",
		"@types/fs-extra": "^11.0.4",
		"@types/inquirer": "^9.0.7",
		"@types/jsdom": "^21.1.7",
		"@types/lodash": "^4.17.7",
		"@types/node": "^22.5.2",
		"@types/validator": "^13.12.1",
		"@vitejs/plugin-vue": "^5.1.3",
		"@vitejs/plugin-vue-jsx": "^4.0.1",
		"@vue/eslint-config-prettier": "^9.0.0",
		"@vue/eslint-config-typescript": "^13.0.0",
		"@vue/test-utils": "^2.4.6",
		"@vue/tsconfig": "^0.5.1",
		"autoprefixer": "^10.4.20",
		"cypress": "^13.14.1",
		"eslint": "^9.9.1",
		"eslint-plugin-cypress": "^3.5.0",
