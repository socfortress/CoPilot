	},
	"optionalDependencies": {
		"@rollup/rollup-linux-x64-gnu": "^4.21.2"
	},
	"overrides": {
		"@vue/eslint-config-typescript": {
			"eslint": "^9.9.1"
		},
		"@typescript-eslint/type-utils": {
			"eslint": "^9.9.1"
		},
		"@typescript-eslint/utils": {
			"eslint": "^9.9.1"
		},
		"secure-ls": {
			"crypto-js": "^4.2.0"
		},
		"micromatch": "^4.0.8",
		"ws": "^8.18.0"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
